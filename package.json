{
  "name": "resa-squash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "prod": "TEAMR_API_KEY=$(cat .env.production | grep TEAMR_API_KEY | cut -d '=' -f2) NODE_ENV=production npm run build && TEAMR_API_KEY=$(cat .env.production | grep TEAMR_API_KEY | cut -d '=' -f2) NODE_ENV=production npm run start",
    "build-prod": "TEAMR_API_KEY=$(cat .env.production | grep TEAMR_API_KEY | cut -d '=' -f2) NODE_ENV=production npm run build",
    "build": "NEXT_PUBLIC_BUILD_TIME=$(date -u +\"%Y-%m-%dT%H:%M:%SZ\") next build",
    "start": "next start",
    "lint": "next lint",
    "init-db": "node scripts/init-db.mjs",
    "postbuild": "node scripts/migrate.mjs"
  },
  "dependencies": {
    "@types/js-cookie": "^3.0.6",
    "@vercel/postgres": "^0.5.1",
    "date-fns": "^4.1.0",
    "js-cookie": "^3.0.5",
    "next": "15.1.6",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/pg": "^8.11.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
